{
    "$schema": "https://turborepo.org/schema.json",
    "baseBranch": "origin/main",
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                ".next/**"
            ]
        },
        "test": {
            "dependsOn": [
                "^build"
            ],
            "outputs": []
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "cache": false
        },
        "devf": {
            "cache": false
        },
        "devc": {
            "cache": false
        },
        "devs": {
            "cache": false
        },
        "chain": {
            "cache": false
        },
        "deployh": {
            "cache": false
        },
        "deployc": {
            "cache": false
        }
    }
}